{
    "$schema": "https://ev2schema.azure.net/schemas/2020-01-01/rolloutSpecification.json",
    "contentVersion": "1.0.0.0",
    "RolloutMetadata": {
      "ServiceModelPath": "drop/Ev2/ServiceModel.Test.json",
      "ScopeBindingsPath": "drop/Ev2/ScopeBindings.Test.json",
      "Name": "Dusseldorf Image Upload",
      "RolloutType": "Major",
      "BuildSource": {
        "Parameters": {
          "VersionFile": "drop/ev2/buildver.txt"
        }
      },
      "Notification": {
        "Email": {
          "To": "dusseldorks@microsoft.com"
        }
      }
    },
    "OrchestratedSteps": [
        {
        "Name": "CreateInfrastructure",
        "TargetType": "ServiceResource",
        "TargetName": "CreateInfrastructureResource",
