      },
      {
        "Name": "UploadLinuxContainer",
        "TargetType": "ServiceResource",
        "TargetName": "LinuxContainerUpload",
        "Actions": ["Shell/Run"],
        "DependsOn": [ "CreateInfrastructure"]
      }
    ]
