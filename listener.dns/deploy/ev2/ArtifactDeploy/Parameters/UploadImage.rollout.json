{
    "$schema": "https://ev2schema.azure.net/schemas/2020-01-01/rolloutParameters.json",
    "contentVersion": "1.0.0.0",
    "shellExtensions": [
      {
        "name": "Run",
        "type": "Run",
        "properties": {
          "maxExecutionTime": "PT50M"
        },
        "package": {
          "reference": {
            "path": "drop/image.tar"
          }
        },
        "launch": {
          "command": [
            "/bin/bash",
            "/package/unarchive/deploy/ev2/scripts/push-image.sh"
          ],
          "environmentVariables": [
            {
              "name": "DESTINATION_ACR_NAME",
              "value": "dusseldorftest"
            },
            {
              "name": "TARBALL_IMAGE_FILE_SAS",
              "reference": {
                "path": "drop/dusseldorf.listener.dns.tar"
              },
              "asSecureValue": "true"
            },
            {
              "name": "DESTINATION_FILE_NAME",
              "value": "dusseldorf.listener.dns.tar"
            },
            {
              "name": "IMAGE_NAME",
              "value": "dusseldorf.listener.dns"
            },
            {
              "name": "TAG_NAME",
              "value": "__BUILD_VERSION__"
            }
          ],
          "identity": {
            "type": "userAssigned",
            "userAssignedIdentities": [
              "/subscriptions/__SUBSCRIPTION_ID__/resourcegroups/__RESOURCE_GROUP__/providers/Microsoft.ManagedIdentity/userAssignedIdentities/dusseldorfimagepush"
            ]
          }
        }
      }
    ]
  }